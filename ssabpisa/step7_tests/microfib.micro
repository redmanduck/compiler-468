PROGRAM microfib
BEGIN
	STRING screw := "F(n): ";
	STRING eol := "\n";
	STRING neq := "n = ";
	
	FUNCTION INT F (INT n)
	BEGIN
		INT ret1,ret2;
		IF (n > 2)
			ret1 := F(n-1);
			ret2 := F(n-2);
			RETURN ret1+ret2;
		ENDIF
		IF (n = 0)   --This is a comment
			RETURN 0;
		ELSE
			RETURN 1;
		ENDIF
	END
	
	
	FUNCTION INT main ()
	BEGIN
		INT i, end, result;
		WRITE(screw);
		READ(i);

		result := F(i);
		WRITE(result);


		RETURN 0;
	END

END	
