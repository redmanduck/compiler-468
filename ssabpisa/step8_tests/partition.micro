PROGRAM ipartition
BEGIN 
    FUNCTION INT partition (INT sum, INT lnum)
    BEGIN
        IF (lnum = 0) 
            RETURN 0;
        ENDIF
        IF (sum = 0)
            RETURN 1;
        ENDIF
        IF (sum < 0)
            RETURN 0;
        ENDIF

        RETURN partition(sum, lnum - 1) + partition(sum - lnum, lnum);
    END

    FUNCTION INT main() 
    BEGIN 
        INT sum;
        INT lnum;
        INT k;

        sum := 100;
        lnum := 99;
        k := partition(sum, lnum);
        WRITE(k);
    END 
END